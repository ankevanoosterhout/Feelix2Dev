{
  "name": "feelix",
  "productName": "Feelix",
  "version": "3.0.1",
  "main": "electron/main.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rebuild": "electron-rebuild -f -w serialport",
    "electron": "ng build --base-href ./ && electron .",
    "bobbie": "npm-run-all -p ng:serve electron:serve",
    "ng:serve": "ng serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && electron . --serve",
    "package-mac": "electron-packager . --overwrite --platform=darwin --asar --arch=x64 --icon=src/assets/icons/mac/icon.icns --prune --out=release-builds",
    "package-win": "electron-packager . feelix --overwrite --asar --platform=win32 --arch=x64 --icon=src/assets/icons/win/icon.ico --prune --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Feelix\"",
    "package-linux": "electron-packager . feelix --overwrite --asar --platform=linux --arch=x64 --icon=src/assets/icons/png/512x512.png --prune --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@tensorflow/tfjs": "^3.21.0",
    "@types/uuid": "^3.4.11",
    "@types/webgl2": "^0.0.6",
    "clone": "^2.1.2",
    "closed-chain-ik": "^0.0.3",
    "d3": "^7.6.1",
    "d3-color": "^3.1.0",
    "drag-controls": "^1.0.4",
    "electron-browser-storage": "^1.0.6",
    "file-saver": "^2.0.5",
    "file-system": "^2.2.2",
    "jsonfile": "^6.1.0",
    "jszip": "^3.7.0",
    "ngx-color-picker": "^7.5.0",
    "ngx-electron": "^2.2.0",
    "ngx-filesaver": "^16.0.0",
    "ngx-fs": "0.0.1",
    "ngx-webstorage": "^10.0.1",
    "rxjs": "^7.8.1",
    "serialport": "^11.0.1",
    "three": "^0.138.3",
    "three-ik": "^0.1.0",
    "tslib": "^1.10.0",
    "urdf-loader": "^0.9.2",
    "uuid": "^9.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "^14.2.12",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@electron/rebuild": "^3.3.0",
    "@types/electron": "^1.6.10",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.5.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "electron": "^24.8.3",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^17.1.2",
    "electron-reload": "1.5.0",
    "electron-wix-msi": "^3.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "npm-run-all": "4.1.5",
    "protractor": "^7.0.0",
    "ts-node": "8.3.0",
    "tslint": "~6.1.3",
    "typescript": "4.6.4",
    "wait-on": "3.3.0"
  },
  "optionalDependencies": {
    "exe-icon-extractor": "^1.0.8"
  }
}
